{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "twitch": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "externalAccessToken": {
          "type": "string",
          "default": "ACCESS_TOKEN"
        },
        "externalRefreshToken": {
          "type": "string",
          "default": "REFRESH_TOKEN"
        },
        "externalClientID": {
          "type": "string",
          "default": "CLIENT_ID"
        },
        "clientID": {
          "type": "string",
          "default": "CLIENT_ID"
        },
        "clientSecret": {
          "type": "string",
          "default": "CLIENT_SECRET"
        }
      }
    }
  }
}
